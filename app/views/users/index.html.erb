<% title "User" %>
<table id="main_table" border="0" cellpadding="0" cellspacing="0" class="pretty">
<thead>
  <tr>
    <th>User ID</th>
    <th>Username</th>
    <th>Email</th>
    <th>Lake</th>
    <th>Lake2</th>
    <th>Lake3</th>
    <th>Lake4</th>
    <th>Lake5</th>
  </tr>
</thead>
<tbody>
  <% for user in @users %>
    <tr>
      <td><%=h user.id %></td>
      <td><%=h user.username %></td>
      <td><%=h user.email %></td>
      <td><%=h user.lake1 %></td>
      <td><%=h user.lake2 %></td>
      <td><%=h user.lake3 %></td>
      <td><%=h user.lake4 %></td>
      <td><%=h user.lake5 %></td>
      <td><%= link_to "Edit", edit_user_path(user) %></td>
      <td><%= link_to "Destroy", user, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</tbody>
</table>

<p><%= link_to "New User", new_user_path %></p>
